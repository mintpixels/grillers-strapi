{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "strapi-import-export": {
      "idField": "medusa_product_id"
    }
  },
  "attributes": {
    "medusa_product_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Title": {
      "type": "string"
    },
    "FeaturedImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "GalleryImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Metadata": {
      "type": "component",
      "repeatable": false,
      "component": "pdp.product-metadata"
    },
    "Recipes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recipe.recipe"
    },
    "Categorization": {
      "type": "component",
      "repeatable": false,
      "component": "pdp.sategorization"
    },
    "MedusaProduct": {
      "displayName": "MedusaProduct",
      "type": "component",
      "repeatable": false,
      "component": "pdp.medusa-product"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "SocialMeta": {
      "type": "component",
      "repeatable": false,
      "component": "shared.social-meta"
    }
  }
}