{
  "collectionName": "components_shared_seos",
  "info": {
    "name": "Seo",
    "icon": "search",
    "displayName": "SEO",
    "description": "Core SEO settings for search engines and AI indexing"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true,
      "maxLength": 60,
      "minLength": 1
    },
    "metaDescription": {
      "type": "text",
      "required": true,
      "maxLength": 160,
      "minLength": 50
    },
    "keywords": {
      "type": "string",
      "required": false
    },
    "canonicalUrl": {
      "type": "string",
      "required": false
    },
    "metaRobots": {
      "type": "enumeration",
      "enum": [
        "index, follow",
        "noindex, follow",
        "index, nofollow",
        "noindex, nofollow"
      ],
      "default": "index, follow",
      "required": true
    },
    "structuredData": {
      "type": "json",
      "required": false
    },
    "metaViewport": {
      "type": "string",
      "required": false,
      "default": "width=device-width, initial-scale=1"
    }
  }
}
